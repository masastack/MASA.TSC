@namespace Masa.Tsc.Web.Admin.Rcl.Pages.Components
@inherits TscComponentBase
<MCard>
    <MCardText Class="d-flex pa-0">
        <MTabs SliderColor="white" Class="flex-row font-weight-bold  text-md-h4 col-3">
            <MTab IsActive>All</MTab>
            <MTab>Operator</MTab>
            <MTab>Basic Ability</MTab>
            <MTab>Data Factory</MTab>
            <MTab>Data Factory2</MTab>
        </MTabs>
        <MAutocomplete Class="flex-row col-1 pa-0" BackgroundColor="white" Items="Projects"
                       TItem="ProjectDto" TValue="string" TItemValue="string" ItemValue="r=>r.Name"
                       ItemText="r=>r.Identity">
        </MAutocomplete>
        <SearchTextField Class="flex-row col-5 py-0" BackgroundColor="white"></SearchTextField>
        <SDateTimeRangePicker @bind-StartTime="Start" @bind-EndTime="End" Class="ml-0 pl-0" />
    </MCardText>
</MCard>