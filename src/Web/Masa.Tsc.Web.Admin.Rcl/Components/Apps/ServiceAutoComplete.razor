@inherits TscComponentBase

<SAutoComplete Value=Value
               ValueChanged="(string? val)=>InputValueChangedAsync(val)"
               @key="_key"
               OnBlur=OnBlurAsync
               Items="@(Services?? new())"
               ItemText="app =>Metric?app.Identity:app.Name"
               ItemValue="app => app.Identity"
               Small
               HideSelected=false
               Disabled=Readonly
               Label="@(Label??I18n.T("Service"))"
               BackgroundColor="@(FillBackground?"fill-background":"white")"
               Loading="_isLoading"
               Class="@("rounded-2 " + Class)"
               Style="@Style" />