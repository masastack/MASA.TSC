@namespace Masa.Tsc.Web.Admin.Rcl.Components
@inherits TscComponentBase

<TscTraceSearch OnQueryUpdate="Search"
                QueryServices="QueryServices"
                QueryInstances="QueryInstances"
                QueryEndpoints="QueryEndpoints" />

<TscTraceChart PageMode=PageMode
               OnDateTimeRangeUpdate="Search"
               Data="_chartData" Start="@StartDateTime" End="@EndDateTime" />

<TscTraceList PageMode=PageMode
              Page="_page"
              PageSize="_pageSize"
              OnPaginationUpdate="Search"
              SearchResult="_queryResult"
              SearchLoading="_loading" />