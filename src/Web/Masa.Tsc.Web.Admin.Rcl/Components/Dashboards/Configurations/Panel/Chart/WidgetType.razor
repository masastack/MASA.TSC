<MRow>
    @foreach (var item in Items)
    {
        <MCol Cols="6">
            <MCard OnClick="@(async ()=>await OnSelected(item))" Style="@(item.Selected?"border: 0.5px solid #4318FF;":"")">
                <MImage Height="@("100%")" Width="@("100%")" Style="border-radius: 20px !important;" Contain Src="@item.Src" />
            </MCard>
        </MCol>
    }
</MRow>