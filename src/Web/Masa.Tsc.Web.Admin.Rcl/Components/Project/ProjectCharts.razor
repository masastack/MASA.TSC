        <ErrorWarnChart Title="ERRORS" SubText="errors" Error />
        <ErrorWarnChart Title="WARNS" SubText="warns" Warn />
        <LogTraceChart Trace Title="@("Trace")"/>
    </MCol>
</MRow>
<MRow>
    <MCol Class="d-flex justify-space-between" Style="border:solid 1px red">
        <ObserveChart  Title="OBSTACLES" />
        <GrowthChart />
