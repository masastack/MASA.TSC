    </MCol>
</MRow>
<MRow>
    <MCol Class="d-flex justify-space-between" Style="border:solid 1px red">
       <LogTraceStatiscChart Trace Error/>
        <LogTraceStatiscChart Trace Warn/>
        <LogTraceStatiscChart Log Error/>
        <LogTraceStatiscChart Log Warn/>
    </MCol>
</MRow>