@namespace Masa.Tsc.Web.Admin.Rcl.Pages.Components
<MRow>
    <MCol Cols="6" Class="d-flex align-center">
        <MAutocomplete @bind-Value=@AppId Items=Apps ItemText="r=>r.Name" ItemValue="r=>r.Identity" Class="flex-row col-4 pa-0" Dense Outlined />
        <MLabel Class="pl-2 text-h6">Service:</MLabel><MLabel>@AppId</MLabel>
        <MLabel Class="pl-2 text-h6">ID:</MLabel><MLabel>@AppId</MLabel>
    </MCol>
    <MCol Cols="6" Class="d-flex justify-end align-center">        
        <SDateTimeRangePicker StartTime=@Start EndTime=@End></SDateTimeRangePicker>
        <MSelect @bind-Value="@sasas" Class="col-2"
                 Items="@_selectDataSource"
                 ItemText="u => u"
                 ItemValue="u => u" Dense Outlined>
        </MSelect>
        <MIcon Large Class="px-2">fas fa-rotate</MIcon>
        <MButton Outlined>查看/详情</MButton>
    </MCol>
</MRow>